{
  "expo": {
    "name": "ai-screen-mockups",
    "slug": "ai-screen-mockups",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/sm-icon.png",
    "scheme": "screenmockups",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.rodgetech.screenmockups",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon-transparent.png",
          "backgroundColor": "#1E1E2E",
          "dark": {
            "image": "./assets/images/icon-transparent.png",
            "backgroundColor": "#1E1E2E"
          }
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "organization": "rodgetech",
          "project": "screen-mockups",
          "url": "https://sentry.io/"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "56e6bdc8-8106-4cae-935f-2ef2758c5ba1"
      }
    }
  }
}
